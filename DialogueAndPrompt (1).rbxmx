						<ProtectedString name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")

local QuestSystem = ReplicatedStorage:WaitForChild("QuestSystem")
local Config = QuestSystem:WaitForChild("Config")
local Remotes = QuestSystem:WaitForChild("Remotes")

local RarityConfig = require(Config.RarityConfig)
local UIConfig = require(Config.UIConfig)

local ShowContractUI = Remotes:WaitForChild("ShowContractUI")
local AcceptContract = Remotes:WaitForChild("AcceptContract")

local screenGui = script.Parent
local board = screenGui:WaitForChild("ContractBoard")
local header = board:WaitForChild("Header")
local body = board:WaitForChild("Body")
local footer = board:WaitForChild("Footer")

local rarityBadge = header:WaitForChild("RarityBadge")
local questName = header:WaitForChild("QuestName")
local dayLabel = header:WaitForChild("DayLabel")
local closeButton = header:WaitForChild("CloseButton")
local shimmer = header:WaitForChild("Shimmer")

local description = body:WaitForChild("Description")
local objectives = body:WaitForChild("Objectives")
local objectivesLayout = objectives:WaitForChild("UIListLayout")
local flavorLine = body:WaitForChild("FlavorLine")

local threatBar = footer:WaitForChild("ThreatBar")
local threatFill = threatBar:WaitForChild("Fill")
local threatLabel = footer:WaitForChild("ThreatLabel")
local threatValue = footer:WaitForChild("ThreatValue")
local rewardsLabel = footer:WaitForChild("RewardsLabel")
local acceptButton = footer:WaitForChild("AcceptButton")

local boardGradient = board:FindFirstChildOfClass("UIGradient")
local pulseTween
local driftTween
local shimmerActive = false
local currentContractId

local function applyUIConfig()
	board.Size = UDim2.fromOffset(UIConfig.Panel.Width, UIConfig.Panel.Height)
	rarityBadge.Font = UIConfig.Fonts.Body
	rarityBadge.TextSize = UIConfig.TextSizes.Small
	questName.Font = UIConfig.Fonts.Header
	questName.TextSize = UIConfig.TextSizes.Header
	dayLabel.Font = UIConfig.Fonts.Body
	dayLabel.TextSize = UIConfig.TextSizes.Small
	description.Font = UIConfig.Fonts.Body
	description.TextSize = UIConfig.TextSizes.Body
	flavorLine.Font = UIConfig.Fonts.Body
	flavorLine.TextSize = UIConfig.TextSizes.Small
	threatLabel.Font = UIConfig.Fonts.Body
	threatLabel.TextSize = UIConfig.TextSizes.Small
	threatValue.Font = UIConfig.Fonts.Mono
	threatValue.TextSize = UIConfig.TextSizes.Small
	rewardsLabel.Font = UIConfig.Fonts.Body
	rewardsLabel.TextSize = UIConfig.TextSizes.Small
	acceptButton.Font = UIConfig.Fonts.Header
	acceptButton.TextSize = UIConfig.TextSizes.Small
end

local function setupMobileScale()
	if not UIConfig.Mobile.UseUIScale then
		return
	end
	local uiScale = board:FindFirstChildOfClass("UIScale") or Instance.new("UIScale")
	uiScale.Parent = board
	local reference = Vector2.new(1920, 1080)
	local function updateScale()
		local size = screenGui.AbsoluteSize
		local scaleValue = math.min(size.X / reference.X, size.Y / reference.Y)
		uiScale.Scale = math.clamp(scaleValue, UIConfig.Mobile.MinScale, 1)
	end
	updateScale()
	screenGui:GetPropertyChangedSignal("AbsoluteSize"):Connect(updateScale)
end

applyUIConfig()
setupMobileScale()

local function clearObjectives()
	for _, child in ipairs(objectives:GetChildren()) do
		if child:IsA("TextLabel") then
			child:Destroy()
		end
	end
end

local function stopEffects()
	if pulseTween then
		pulseTween:Cancel()
		pulseTween = nil
	end
	if driftTween then
		driftTween:Cancel()
		driftTween = nil
	end
	shimmerActive = false
	shimmer.Visible = false
end

local function startShimmer()
	shimmerActive = true
	shimmer.Visible = true

	local function runOnce()
		shimmer.Position = UDim2.new(-0.3, 0, 0, 0)
		local tween = TweenService:Create(shimmer, TweenInfo.new(UIConfig.Animations.ShimmerTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
			Position = UDim2.new(1.1, 0, 0, 0),
		})
		tween:Play()
		tween.Completed:Wait()
	end

	task.spawn(function()
		while shimmerActive do
			runOnce()
			task.wait(UIConfig.Animations.ShimmerInterval)
		end
	end)
end

local function applyTheme(rarity)
	local data = RarityConfig.Rarities[rarity] or RarityConfig.Rarities.Common
	rarityBadge.Text = data.DisplayName:upper()
	rarityBadge.TextColor3 = data.Theme.Accent
	questName.TextColor3 = Color3.new(1, 1, 1)
	dayLabel.TextColor3 = Color3.new(0.85, 0.85, 0.85)

	board.BackgroundColor3 = data.Theme.BgBottom
	if boardGradient then
		boardGradient.Color = ColorSequence.new({
			ColorSequenceKeypoint.new(0, data.Theme.BgTop),
			ColorSequenceKeypoint.new(1, data.Theme.BgBottom),
		})
	end

	local stroke = board:FindFirstChildOfClass("UIStroke")
	if stroke then
		stroke.Color = data.Theme.Stroke
	end

	stopEffects()

	if data.Effects.GradientDrift and boardGradient then
		driftTween = TweenService:Create(boardGradient, TweenInfo.new(UIConfig.Animations.GradientDriftTime, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1), {
			Rotation = 360,
		})
		driftTween:Play()
	end

	if data.Effects.Pulse then
		pulseTween = TweenService:Create(rarityBadge, TweenInfo.new(UIConfig.Animations.PulseTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
			Size = UDim2.new(0, rarityBadge.Size.X.Offset * UIConfig.Animations.PulseScale, 0, rarityBadge.Size.Y.Offset * UIConfig.Animations.PulseScale),
		})
		pulseTween:Play()
	end

	if data.Effects.Shimmer then
		startShimmer()
	end
end

local function setAcceptedState(accepted)
	if accepted then
		acceptButton.Text = "CONTRACT ACCEPTED"
		acceptButton.AutoButtonColor = false
		acceptButton.BackgroundTransparency = 0.35
		acceptButton.Active = false
	else
		acceptButton.Text = "ACCEPT CONTRACT"
		acceptButton.AutoButtonColor = true
		acceptButton.BackgroundTransparency = 0
		acceptButton.Active = true
	end
end

local function openBoard()
	board.Visible = true
	board.Position = UDim2.new(0.5, 0, 0.52, 0)
	board.AnchorPoint = Vector2.new(0.5, 0.5)
	board.BackgroundTransparency = 0.1
	local tween = TweenService:Create(board, TweenInfo.new(UIConfig.Animations.OpenTime, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		Position = UDim2.new(0.5, 0, 0.5, 0),
	})
	tween:Play()
end

local function closeBoard()
	stopEffects()
	local tween = TweenService:Create(board, TweenInfo.new(UIConfig.Animations.CloseTime, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
		Position = UDim2.new(0.5, 0, 0.55, 0),
		BackgroundTransparency = 0.15,
	})
	tween:Play()
	tween.Completed:Wait()
	board.Visible = false
	screenGui.Enabled = false
end

ShowContractUI.OnClientEvent:Connect(function(data)
	if not data then
		return
	end

	currentContractId = data.Id
	screenGui.Enabled = true

	questName.Text = data.Name
	dayLabel.Text = string.format("DAY CONTRACT  %d", data.DayIndex)
	description.Text = data.Description

	clearObjectives()
	for _, objective in ipairs(data.Objectives or {}) do
		local label = Instance.new("TextLabel")
		label.BackgroundTransparency = 1
		label.Size = UDim2.new(1, -10, 0, 18)
		label.Font = UIConfig.Fonts.Body
		label.TextSize = UIConfig.TextSizes.Body
		label.TextColor3 = Color3.fromRGB(220, 220, 220)
		label.TextXAlignment = Enum.TextXAlignment.Left
		label.Text = " " .. objective
		label.Parent = objectives
	end

	objectives.CanvasSize = UDim2.new(0, 0, 0, objectivesLayout.AbsoluteContentSize.Y + 6)

	local threat = data.Threat or 0
	threatFill.Size = UDim2.new(math.clamp(threat / 100, 0, 1), 0, 1, 0)
	threatLabel.Text = string.format("THREAT: %s", RarityConfig.getThreatTier(threat))
	threatValue.Text = tostring(threat)

	rewardsLabel.Text = string.format("Rewards: %d survival + %d completion + %.2f/stud", data.Rewards.Survival, data.Rewards.Completion, data.Rewards.Distance)

	if data.FlavorLine and data.FlavorLine ~= "" then
		flavorLine.Text = data.FlavorLine
		flavorLine.Visible = true
	else
		flavorLine.Text = ""
		flavorLine.Visible = false
	end

	applyTheme(data.Rarity)
	setAcceptedState(data.Accepted)

	openBoard()
end)

acceptButton.MouseButton1Click:Connect(function()
	if currentContractId then
		AcceptContract:FireServer(currentContractId)
		setAcceptedState(true)
	end
end)

closeButton.MouseButton1Click:Connect(closeBoard)]]></ProtectedString>
