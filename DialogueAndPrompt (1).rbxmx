				<Item class="Script" referent="RBXF1367D689AA14255A434B22F754E4A3A">
					<Properties>
						<ProtectedString name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Lighting = game:GetService("Lighting")

local DialogModule = require(ReplicatedStorage.DialogModule)

local QuestSystem = ReplicatedStorage:WaitForChild("QuestSystem")
local Config = QuestSystem:WaitForChild("Config")
local State = QuestSystem:WaitForChild("State")
local Remotes = QuestSystem:WaitForChild("Remotes")

local TimeConfig = require(Config.TimeConfig)
local DialogueConfig = require(Config.DialogueConfig)
local QuestContractsConfig = require(Config.QuestContractsConfig)

local RequestContractUI = Remotes:WaitForChild("RequestContractUI")
local ActiveContractId = State:WaitForChild("ActiveContractId")

local npc = script.Parent
local prompt = npc:WaitForChild("ProximityPrompt")

local dialogObject = DialogModule.new(DialogueConfig.NPCName, npc, prompt)

local currentDialogData = nil

local function isDaytime()
	local minutes = Lighting:GetMinutesAfterMidnight()
	return minutes >= TimeConfig.DayStartMinutes and minutes < TimeConfig.DayEndMinutes
end

local function getActiveContract()
	for _, contract in ipairs(QuestContractsConfig) do
		if contract.Id == ActiveContractId.Value then
			return contract
		end
	end
	return nil
end

prompt.Triggered:Connect(function(player)
	local isDay = isDaytime()
	local dialogData = isDay and DialogueConfig.DayLines or DialogueConfig.NightLines
	currentDialogData = dialogData

	local introLines = dialogData.Intro
	local contract = isDay and getActiveContract()
	if contract and contract.Rarity == "Legendary" then
		local flair = DialogueConfig.RarityFlair and DialogueConfig.RarityFlair.Legendary
		if flair and #flair > 0 then
			introLines = flair
		end
	end

	local intro = introLines[math.random(1, #introLines)]

	dialogObject.dialogs = {}
	dialogObject.dialogOption = 1
	dialogObject:addDialog(intro, dialogData.Responses)
	dialogObject:triggerDialog(player, 1)
end)

dialogObject.responded:Connect(function(responseNum)
	if not currentDialogData then
		return
	end

	local isDay = isDaytime()
	if isDay and responseNum == (currentDialogData.ContractResponseIndex or 1) then
		RequestContractUI:FireServer()
		dialogObject:hideGui(DialogueConfig.DayLines.AcceptLine or "Contract accepted.")
	else
		dialogObject:hideGui(currentDialogData.ExitLine or "Stay safe out there.")
	end
end)]]></ProtectedString>
