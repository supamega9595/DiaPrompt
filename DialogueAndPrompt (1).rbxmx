	return tween
end

local function stopCountConnection()
	if countConnection then
		countConnection:Disconnect()
		countConnection = nil
	end
end

local function pulseQuestValue()
	local scale = UI.QuestValue:FindFirstChild("PulseScale")
	if not scale then
		scale = Instance.new("UIScale")
		scale.Name = "PulseScale"
		scale.Parent = UI.QuestValue
	end
	scale.Scale = 1
	local tweenOut = TweenService:Create(scale, TweenInfo.new(0.14, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Scale = 1.06})
	local tweenIn = TweenService:Create(scale, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Scale = 1})
	tweenOut:Play()
	tweenOut.Completed:Connect(function()
		tweenIn:Play()
	end)
end

local function startFooterDots()
	if footerLoopActive then
		return
	end
	footerLoopActive = true
	task.spawn(function()
		local dots = {"", ".", "..", "..."}
		local index = 1
		while footerLoopActive do
			UI.Footer.Text = "Returning at dawn" .. dots[index]
			index = index % #dots + 1
			task.wait(0.4)
		end
	end)
end

local function stopFooterDots()
	footerLoopActive = false
end

local function showSummary(data)
	buildUi()
	gui.Enabled = true
	UI.Panel.Visible = true
	UI.PanelScale.Scale = 0.05
	setTextAlpha(0)
	startFooterDots()

	local survived = data and data.Survived
	local completed = data and data.QuestCompleted
	local distance = data and (data.DistanceStuds or data.Distance) or 0
	local cashEarned = data and (data.CashEarned or data.CashAwarded) or 0
	local xpEarned = data and (data.XPEarned or data.XPAwarded) or 0
	local winsEarned = data and (data.WinsEarned or data.WinsAwarded) or 0

	UI.SurvivedValue.Text = survived and "YES" or "NO"
	UI.SurvivedValue.TextColor3 = Color3.fromRGB(245, 245, 245)
	UI.SurvivedChip.BackgroundColor3 = survived and Color3.fromRGB(46, 76, 58) or Color3.fromRGB(70, 64, 64)

	UI.QuestValue.Text = completed and "YES" or "NO"
	UI.QuestValue.TextColor3 = Color3.fromRGB(245, 245, 245)
	UI.QuestChip.BackgroundColor3 = completed and Color3.fromRGB(90, 74, 35) or Color3.fromRGB(70, 64, 64)

	UI.DistanceValue.Text = string.format("<font size='20'>%s</font> <font size='14' color='#b6b0a8'>studs</font>", formatNumber(distance))
	UI.DistanceValue.TextColor3 = COLORS.Text

	UI.CashValue.Text = "+0"
	UI.XPValue.Text = "+0"
	UI.WinsValue.Text = "+0"

	local openTween = tweenScale(1, Enum.EasingStyle.Back, Enum.EasingDirection.Out, OPEN_TIME)

	openTween.Completed:Connect(function()
		ensureSound("OpenSound", "rbxassetid://76917399147672"):Play()
		if completed then
			pulseQuestValue()
		end
		task.delay(COUNT_DELAY, function()
			local countSound = ensureSound("CountSound", "rbxassetid://140677742495553")
			countSound:Play()
			local start = os.clock()
			stopCountConnection()
			countConnection = RunService.RenderStepped:Connect(function()
				local elapsed = os.clock() - start
				local alpha = math.clamp(elapsed / COUNT_DURATION, 0, 1)
				local cashValue = math.floor(cashEarned * alpha)
				local xpValue = math.floor(xpEarned * alpha)
				local winsValue = math.floor(winsEarned * alpha)
				UI.CashValue.Text = "+" .. formatNumber(cashValue)
				UI.XPValue.Text = "+" .. formatNumber(xpValue)
				UI.WinsValue.Text = "+" .. formatNumber(winsValue)
				if alpha >= 1 then
					stopCountConnection()
					countSound:Stop()
					task.delay(HOLD_DURATION, function()
						local closeSound = ensureSound("CloseSound", "rbxassetid://73730549515939")
						closeSound:Play()
						local closeTween = tweenScale(0.05, Enum.EasingStyle.Quad, Enum.EasingDirection.In, CLOSE_TIME)
						closeTween.Completed:Connect(function()
							stopFooterDots()
							UI.Panel.Visible = false
							gui.Enabled = false
						end)
					end)
				end
			end)
		end)
	end)
end

