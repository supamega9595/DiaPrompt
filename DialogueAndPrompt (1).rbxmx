							<Item class="Script" referent="RBXDA26CF958DDE42959339D4BEDA369C1E">
								<Properties>
									<ProtectedString name="Source"><![CDATA[--Click to Open/Close Drawer--
local pc = script.Parent.PrismaticConstraint
local clicker = script.Parent.Clicker
local proximityPrompt = clicker.ProximityPrompt
local drawerClose = clicker.DrawerClose
local drawerOpen = clicker.DrawerOpen
local open = false

game:GetService("SoundService")

proximityPrompt.Triggered:Connect(function()
	print("Drawer Clicked")
	open = not open

	pc.TargetPosition = open and 1.75 or 0
	print("SoundSection")
	if open == false then
		local copydrawerOpen = drawerOpen:Clone()
		copydrawerOpen.Parent = clicker
		copydrawerOpen.Ended:Connect(function() copydrawerOpen:Destroy() end)
		copydrawerOpen:Play()
		print("SoundClose")
	else
		local copydrawerClose = drawerClose:Clone()
		copydrawerClose.Parent = clicker
		copydrawerClose.Ended:Connect(function() copydrawerClose:Destroy() end)
		copydrawerClose:Play()
		print("SoundOpen")
	end

end)]]></ProtectedString>
