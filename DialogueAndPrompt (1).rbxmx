		TweenService:Create(inputFrame, uiTweenInfo, { Position = UDim2.new(0.35, 0, 0.5, 0) }):Play()
		TweenService:Create(textFrame, uiTweenInfo, { GroupTransparency = 0 }):Play()
	end
