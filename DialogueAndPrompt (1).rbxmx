local function isDaytime(minutes)
	return minutes >= TimeConfig.DayStartMinutes and minutes < TimeConfig.DayEndMinutes
end

local function getContractById(contractId)
	for _, contract in ipairs(QuestContractsConfig) do
		if contract.Id == contractId then
			return contract
		end
	end
	return nil
end

local function chooseDailyContract()
	if QuestSettings.ForceContract then
		local forced = getContractById(QuestSettings.ForcedContractId)
		if not forced then
			warn("QuestService: Forced contract id not found:", QuestSettings.ForcedContractId)
		else
			return forced
		end
	end
	if #QuestContractsConfig == 0 then
		warn("QuestContractsConfig has no contracts configured.")
		return nil
	end
	local index = math.random(1, #QuestContractsConfig)
	return QuestContractsConfig[index]
end

local questState = {
	activeContract = nil,
	progress = 0,
	target = 0,
	completed = false,
	connections = {},
	startTime = 0,
	playerDistances = {},
	playerLastPositions = {},
	playerDied = {},
	playerDeathConnections = {},
	activePages = {},
}

local function clearQuestConnections()
	for _, connection in ipairs(questState.connections) do
		connection:Disconnect()
	end
	questState.connections = {}
end

local function getPlayerFromHit(hit)
	if not hit then
		return nil
	end
	local character = hit:FindFirstAncestorOfClass("Model")
	if not character then
		return nil
	end
	return Players:GetPlayerFromCharacter(character)
end

local function getPickupBasePart(instance)
	if instance:IsA("ProximityPrompt") then
		local parent = instance.Parent
		if parent and parent:IsA("BasePart") then
			return parent
		elseif parent and parent:IsA("Model") then
			return parent.PrimaryPart or parent:FindFirstChildWhichIsA("BasePart")
		end
	elseif instance:IsA("BasePart") then
		return instance
	elseif instance:IsA("Model") then
		return instance.PrimaryPart or instance:FindFirstChildWhichIsA("BasePart")
	end
	return nil
end

local function getQuestPagesFolder()
	local questSpawns = workspace:FindFirstChild(QuestSettings.Pages.FolderName)
	if not questSpawns then
		return nil
	end
	return questSpawns:FindFirstChild(QuestSettings.Pages.PagesFolderName)
end

local function recordPartDefaults(part)
	if part:GetAttribute("QuestDefaultTransparency") == nil then
		part:SetAttribute("QuestDefaultTransparency", part.Transparency)
	end
	if part:GetAttribute("QuestDefaultCanCollide") == nil then
		part:SetAttribute("QuestDefaultCanCollide", part.CanCollide)
	end
	if part:GetAttribute("QuestDefaultCanTouch") == nil then
		part:SetAttribute("QuestDefaultCanTouch", part.CanTouch)
	end
	if part:GetAttribute("QuestDefaultCanQuery") == nil then
		part:SetAttribute("QuestDefaultCanQuery", part.CanQuery)
	end
end

local function recordSurfaceDefaults(surface)
	if surface:GetAttribute("QuestDefaultTransparency") == nil then
		surface:SetAttribute("QuestDefaultTransparency", surface.Transparency)
	end
end

local function setSurfaceVisibility(surface, isActive)
	recordSurfaceDefaults(surface)
	if isActive then
		surface.Transparency = surface:GetAttribute("QuestDefaultTransparency") or surface.Transparency
	else
		surface.Transparency = 1
	end
end

local function setPartVisibility(part, isActive)
	recordPartDefaults(part)
	if isActive then
		part.Transparency = part:GetAttribute("QuestDefaultTransparency") or part.Transparency
		part.CanCollide = part:GetAttribute("QuestDefaultCanCollide") ~= false
		part.CanTouch = part:GetAttribute("QuestDefaultCanTouch") ~= false
		part.CanQuery = part:GetAttribute("QuestDefaultCanQuery") ~= false
	else
		part.Transparency = 1
		part.CanCollide = false
		part.CanTouch = false
		part.CanQuery = false
	end

	for _, child in ipairs(part:GetChildren()) do
		if child:IsA("Decal") or child:IsA("Texture") then
			setSurfaceVisibility(child, isActive)
		end
	end
end

local function setPromptVisibility(prompt, isActive)
	if prompt:GetAttribute("QuestDefaultEnabled") == nil then
		prompt:SetAttribute("QuestDefaultEnabled", prompt.Enabled)
	end
	if isActive then
		prompt.Enabled = prompt:GetAttribute("QuestDefaultEnabled") ~= false
	else
		prompt.Enabled = false
	end
end

local function setPageActive(page, isActive)
	local parts = {}
	if page:IsA("BasePart") then
		table.insert(parts, page)
	else
		for _, descendant in ipairs(page:GetDescendants()) do
			if descendant:IsA("BasePart") then
				table.insert(parts, descendant)
			end
		end
	end
	for _, part in ipairs(parts) do
		setPartVisibility(part, isActive)
	end

	local prompts = {}
	if page:IsA("ProximityPrompt") then
		table.insert(prompts, page)
	else
		for _, descendant in ipairs(page:GetDescendants()) do
			if descendant:IsA("ProximityPrompt") then
				table.insert(prompts, descendant)
			end
		end
	end
	for _, prompt in ipairs(prompts) do
		setPromptVisibility(prompt, isActive)
	end
end

local function findPageRoot(instance)
	local folder = getQuestPagesFolder()
	if not folder then
		return nil
	end
	local node = instance
	while node and node ~= workspace do
		if node.Parent == folder then
			return node
		end
		node = node.Parent
	end
	return nil
end

local function removeQuestPickupTags(page)
	if CollectionService:HasTag(page, QuestSettings.Pages.PickupTag) then
		CollectionService:RemoveTag(page, QuestSettings.Pages.PickupTag)
	end
	for _, descendant in ipairs(page:GetDescendants()) do
		if CollectionService:HasTag(descendant, QuestSettings.Pages.PickupTag) then
			CollectionService:RemoveTag(descendant, QuestSettings.Pages.PickupTag)
		end
	end
end

local function applyQuestPickupTag(page)
	local prompt = page:FindFirstChildOfClass("ProximityPrompt") or page:FindFirstChildWhichIsA("ProximityPrompt", true)
	if prompt then
		CollectionService:AddTag(prompt, QuestSettings.Pages.PickupTag)
		return
	end
	CollectionService:AddTag(page, QuestSettings.Pages.PickupTag)
end

local function resetAllPages()
	local folder = getQuestPagesFolder()
	if not folder then
		return
	end
	for _, page in ipairs(folder:GetChildren()) do
		if page:IsA("BasePart") or page:IsA("Model") then
			removeQuestPickupTags(page)
			setPageActive(page, false)
		end
	end
	questState.activePages = {}
end

local function selectRandomPages()
	local folder = getQuestPagesFolder()
	if not folder then
		return {}
	end
	local pages = {}
	for _, page in ipairs(folder:GetChildren()) do
		if page:IsA("BasePart") or page:IsA("Model") then
			table.insert(pages, page)
		end
	end
	for i = #pages, 2, -1 do
		local j = math.random(1, i)
		pages[i], pages[j] = pages[j], pages[i]
	end
	local count = math.min(QuestSettings.Pages.PagesToSpawn or 8, #pages)
	local selection = {}
	for i = 1, count do
		table.insert(selection, pages[i])
	end
	return selection
end

local function activatePageQuest()
	resetAllPages()
	if isDaytime(Lighting:GetMinutesAfterMidnight()) then
		return
	end
	local selection = selectRandomPages()
	for _, page in ipairs(selection) do
		setPageActive(page, true)
		applyQuestPickupTag(page)
	end
	questState.activePages = selection
end

local function deactivatePageQuest()
	resetAllPages()
end

local function hideCollectedPage(instance)
	local page = findPageRoot(instance) or instance
	if page then
		local part = page:IsA("BasePart") and page or page:FindFirstChildWhichIsA("BasePart")
		if part then
			local sound = Instance.new("Sound")
			sound.Name = "QuestPageCollectSound"
			sound.SoundId = "rbxassetid://131858956380043"
			sound.Volume = 1
			sound.RollOffMode = Enum.RollOffMode.Inverse
			sound.RollOffMaxDistance = 35
			sound.RollOffMinDistance = 6
			sound.Parent = part
			sound:Play()
			sound.Ended:Connect(function()
				sound:Destroy()
			end)
		end
		setPageActive(page, false)
		removeQuestPickupTags(page)
	end
end

local function registerDeathTracking(player)
	local function onCharacterAdded(character)
		local existing = questState.playerDeathConnections[player]
		if existing then
			existing:Disconnect()
			questState.playerDeathConnections[player] = nil
		end
		local humanoid = character:FindFirstChildOfClass("Humanoid") or character:WaitForChild("Humanoid", 5)
		if humanoid then
			local connection
			connection = humanoid.Died:Connect(function()
				questState.playerDied[player] = true
			end)
			questState.playerDeathConnections[player] = connection
		end
	end
	player.CharacterAdded:Connect(onCharacterAdded)
	if player.Character then
		onCharacterAdded(player.Character)
	end
end

local function ensureNpcPrompt(isDay)
	local npc = workspace:FindFirstChild("showcase npc")
	if not npc then
		warn("QuestService: NPC 'showcase npc' not found in Workspace.")
		return
	end

	local templateFolder = workspace:FindFirstChild("Custom Prompts")
	local promptTemplate = templateFolder and templateFolder:FindFirstChild("Prompt1")
	if not promptTemplate then
		warn("QuestService: Prompt1 template not found in Workspace > Custom Prompts.")
		return
	end

	local templatePrompt = promptTemplate:FindFirstChildOfClass("ProximityPrompt")
	if not templatePrompt then
		warn("QuestService: ProximityPrompt missing from Prompt1 template.")
		return
	end

	local existingPrompt = npc:FindFirstChildOfClass("ProximityPrompt")
	if existingPrompt then
		existingPrompt:Destroy()
	end

	local clonedPrompt = templatePrompt:Clone()
	clonedPrompt.Name = "ProximityPrompt"
	clonedPrompt.ActionText = DialogueConfig.PromptActionText
	clonedPrompt.ObjectText = DialogueConfig.PromptObjectText
	clonedPrompt.Parent = npc

	CollectionService:AddTag(clonedPrompt, "NPCprompt")

	local conf = clonedPrompt:FindFirstChild("CustomPromptConf")
	if conf then
		local custom = conf:FindFirstChild("CustomPrompt")
		local nameValue = custom and custom:FindFirstChild("PromptName")
		if nameValue and nameValue:IsA("StringValue") then
			nameValue.Value = isDay and "Prompt1" or "Prompt2"
		end
	end
end

local function updatePromptStyle(isDay)
	local npc = workspace:FindFirstChild("showcase npc")
	if not npc then
		return
	end
	local prompt = npc:FindFirstChildOfClass("ProximityPrompt")
	if not prompt then
		return
	end
	local conf = prompt:FindFirstChild("CustomPromptConf")
	if conf then
		local custom = conf:FindFirstChild("CustomPrompt")
		local nameValue = custom and custom:FindFirstChild("PromptName")
		if nameValue and nameValue:IsA("StringValue") then
			nameValue.Value = isDay and "Prompt1" or "Prompt2"
		end
	end
end

local questHandlers = {}

questHandlers.SharedCollectCount = {
	start = function(contract)
		questState.target = contract.Settings.Count or 1
		questState.progress = 0
		questState.completed = false
		local pickupTag = contract.Settings.PickupTag
		local collected = {}

		local function registerPickup(instance)
			if collected[instance] then
				return
			end
			collected[instance] = true
			if pickupTag == QuestSettings.Pages.PickupTag then
				hideCollectedPage(instance)
			end
			questState.progress += 1
			if questState.progress >= questState.target then
				questState.completed = true
			end
		end

		local function connectInstance(instance)
			local part = getPickupBasePart(instance)
			if not part then
				return
			end
			if instance:IsA("ProximityPrompt") then
				local connection = instance.Triggered:Connect(function()
					registerPickup(instance)
				end)
				table.insert(questState.connections, connection)
			else
				local connection = part.Touched:Connect(function(hit)
					local player = getPlayerFromHit(hit)
					if player then
						registerPickup(instance)
					end
				end)
				table.insert(questState.connections, connection)
			end
		end

		for _, instance in ipairs(CollectionService:GetTagged(pickupTag)) do
			connectInstance(instance)
		end

		local added = CollectionService:GetInstanceAddedSignal(pickupTag):Connect(connectInstance)
		table.insert(questState.connections, added)
	end,
	stop = function()
		clearQuestConnections()
	end,
}

questHandlers.DeliverItem = {
	start = function(contract)
		questState.completed = false
		questState.progress = 0
		questState.target = 1

		local crateTag = contract.Settings.CrateTag
		local dropTag = contract.Settings.DropTag

		local function markPicked(crate, player)
			if not player then
				return
			end
			crate:SetAttribute("QuestCarrierUserId", player.UserId)
		end

		local function connectCrate(crate)
			local part = getPickupBasePart(crate)
			if not part then
				return
			end

			local prompt = crate:FindFirstChildOfClass("ProximityPrompt")
			if prompt then
				local connection = prompt.Triggered:Connect(function(player)
					markPicked(crate, player)
				end)
				table.insert(questState.connections, connection)
			else
				local connection = part.Touched:Connect(function(hit)
					local player = getPlayerFromHit(hit)
					if player then
						markPicked(crate, player)
					end
				end)
				table.insert(questState.connections, connection)
			end
		end

		local function getCrateFromHit(hit)
			local node = hit
			while node and node ~= workspace do
				if CollectionService:HasTag(node, crateTag) then
					return node
				end
				node = node.Parent
			end
			return nil
		end

		local function connectDrop(drop)
			local part = getPickupBasePart(drop)
			if not part then
				return
			end
			local connection = part.Touched:Connect(function(hit)
				local crate = getCrateFromHit(hit)
				if crate and crate:GetAttribute("QuestCarrierUserId") then
					questState.completed = true
					questState.progress = 1
				end
			end)
			table.insert(questState.connections, connection)
		end

		for _, crate in ipairs(CollectionService:GetTagged(crateTag)) do
			connectCrate(crate)
		end
		for _, drop in ipairs(CollectionService:GetTagged(dropTag)) do
			connectDrop(drop)
		end

		table.insert(questState.connections, CollectionService:GetInstanceAddedSignal(crateTag):Connect(connectCrate))
		table.insert(questState.connections, CollectionService:GetInstanceAddedSignal(dropTag):Connect(connectDrop))
	end,
	stop = function()
		clearQuestConnections()
	end,
}

local function startNightlyQuest(contract)
	clearQuestConnections()
	questState.activeContract = contract
	questState.startTime = os.time()
	questState.playerDistances = {}
	questState.playerLastPositions = {}
	for _, player in ipairs(Players:GetPlayers()) do
		questState.playerDied[player] = false
	end

	local handler = questHandlers[contract.Type]
	if handler and handler.start then
		handler.start(contract)
	else
		warn("QuestService: No handler for quest type:", contract.Type)
	end

	if contract.Id == QuestSettings.ForcedContractId or (contract.Settings and contract.Settings.PickupTag == QuestSettings.Pages.PickupTag) then
		activatePageQuest()
	end
end

local function stopNightlyQuest()
	local contract = questState.activeContract
	if contract then
		local handler = questHandlers[contract.Type]
		if handler and handler.stop then
			handler.stop()
		end
	end
	deactivatePageQuest()
	questState.activeContract = nil
	questState.completed = false
	questState.progress = 0
	questState.target = 0
end

local function calculateDistanceCash(distance)
	local cfg = RewardsConfig.Distance
	local softCap = cfg.SoftCapDistance
	local postCapMultiplier = cfg.PostCapMultiplier
	if distance <= softCap then
		return distance * RewardsConfig.DistanceCashPerStud
	end
	local capped = softCap * RewardsConfig.DistanceCashPerStud
	local remaining = distance - softCap
	return capped + (remaining * RewardsConfig.DistanceCashPerStud * postCapMultiplier)
end

local function calculateXP(distance, survived, completed)
	local xpConfig = RewardsConfig.XP or {}
	local total = 0
	if survived then
		total += xpConfig.SurvivalBase or 0
	end
	if completed then
		total += xpConfig.CompletionBonus or 0
	end
	total += distance * (xpConfig.DistancePerStud or 0)
	return math.floor(total)
end

local function awardCash(player, amount)
	local leaderstats = player:FindFirstChild("leaderstats")
	if leaderstats then
		local cash = leaderstats:FindFirstChild("Cash") or leaderstats:FindFirstChild("Money") or leaderstats:FindFirstChild("Coins")
		if cash and (cash:IsA("NumberValue") or cash:IsA("IntValue")) then
			cash.Value += math.floor(amount)
			return
		end
	end
	player:SetAttribute("DailyContractCash", math.floor((player:GetAttribute("DailyContractCash") or 0) + amount))
end

local function awardWins(player, amount)
	local leaderstats = player:FindFirstChild("leaderstats")
	if leaderstats then
		local wins = leaderstats:FindFirstChild("Wins")
		if wins and wins:IsA("IntValue") then
			wins.Value += amount
			return
		end
	end
	player:SetAttribute("DailyContractWins", math.floor((player:GetAttribute("DailyContractWins") or 0) + amount))
end

local function awardXP(player, amount)
	local leaderstats = player:FindFirstChild("leaderstats")
	if leaderstats then
		local xp = leaderstats:FindFirstChild("XP")
		if xp and (xp:IsA("IntValue") or xp:IsA("NumberValue")) then
			xp.Value += math.floor(amount)
			return
		end
	end
	player:SetAttribute("DailyContractXP", math.floor((player:GetAttribute("DailyContractXP") or 0) + amount))
end

local function payoutNightRewards()
	local contract = questState.activeContract
	if not contract then
		return
	end
	local rarityMultiplier = RewardsConfig.MultipliersByRarity[contract.Rarity] or 1

	for _, player in ipairs(Players:GetPlayers()) do
		local distance = questState.playerDistances[player] or 0
		local survived = not questState.playerDied[player]
		local survivalCash = RewardsConfig.SurvivalCashBase
		local completionCash = questState.completed and RewardsConfig.QuestCompleteCash or 0
		local distanceCash = calculateDistanceCash(distance)
		local totalCash = (survivalCash + completionCash + distanceCash) * rarityMultiplier
		local xpAward = calculateXP(distance, survived, questState.completed)
		local winsAward = survived and 1 or 0

		awardCash(player, totalCash)
		if winsAward > 0 then
			awardWins(player, winsAward)
		end
		if xpAward > 0 then
			awardXP(player, xpAward)
		end

		ShowNightSummary:FireClient(player, {
			Survived = survived,
			QuestCompleted = questState.completed,
			Distance = distance,
			CashAwarded = totalCash,
			XPAwarded = xpAward,
			WinsAwarded = winsAward,
		})
	end
end

local function updateDistanceTracking()
	local cfg = RewardsConfig.Distance
	for _, player in ipairs(Players:GetPlayers()) do
		local character = player.Character
		local root = character and character:FindFirstChild("HumanoidRootPart")
		if root then
			local last = questState.playerLastPositions[player]
			if last then
				local delta = (root.Position - last)
				local distance = delta.Magnitude
				local speed = distance
				if speed >= cfg.MinSpeedToCount then
					questState.playerDistances[player] = (questState.playerDistances[player] or 0) + distance
				end
			end
			questState.playerLastPositions[player] = root.Position
		end
	end
end

local function getResolvedContractData(player)
	local contract = getContractById(ActiveContractId.Value)
	if not contract then
		return nil
	end

	local rarityData = RarityConfig.Rarities[contract.Rarity]
	local threat = contract.Threat or (rarityData and rarityData.DefaultThreat) or 0
	local accepted = player:GetAttribute("AcceptedContractId") == contract.Id and player:GetAttribute("AcceptedContractDay") == DayIndex.Value

	return {
		Id = contract.Id,
		Name = contract.Name,
		Description = contract.Description,
		Objectives = contract.Objectives,
		Rarity = contract.Rarity,
		Threat = threat,
		DayIndex = DayIndex.Value,
		Accepted = accepted,
		Rewards = {
			Survival = RewardsConfig.SurvivalCashBase,
			Completion = RewardsConfig.QuestCompleteCash,
			Distance = RewardsConfig.DistanceCashPerStud,
		},
	FlavorLine = DialogueConfig.BoardFlavor,
	}
end

RequestContractUI.OnServerEvent:Connect(function(player)
	local minutes = Lighting:GetMinutesAfterMidnight()
	if not isDaytime(minutes) then
		return
	end

	local data = getResolvedContractData(player)
	if data then
		ShowContractUI:FireClient(player, data)
	end
end)

AcceptContract.OnServerEvent:Connect(function(player, contractId)
	if contractId ~= ActiveContractId.Value then
		return
	end

	local minutes = Lighting:GetMinutesAfterMidnight()
	if not isDaytime(minutes) then
		return
	end

	player:SetAttribute("AcceptedContractId", contractId)
	player:SetAttribute("AcceptedContractDay", DayIndex.Value)
end)

local function rollNewContract()
	local contract = chooseDailyContract()
	if contract then
		ActiveContractId.Value = contract.Id
	end
end

local lastIsDay = isDaytime(Lighting:GetMinutesAfterMidnight())

if lastIsDay then
	DayIndex.Value = math.max(DayIndex.Value, 1)
	if ActiveContractId.Value == "" then
		rollNewContract()
	end
else
	DayIndex.Value = DayIndex.Value == 0 and 0 or DayIndex.Value
end

ensureNpcPrompt(lastIsDay)
resetAllPages()

for _, player in ipairs(Players:GetPlayers()) do
	registerDeathTracking(player)
end

if not lastIsDay then
	local contract = getContractById(ActiveContractId.Value)
	if contract then
		startNightlyQuest(contract)
	end
end

task.spawn(function()
	while true do
		local minutes = Lighting:GetMinutesAfterMidnight()
		local isDay = isDaytime(minutes)

		if isDay ~= lastIsDay then
			if isDay then
				DayIndex.Value += 1
				rollNewContract()
				updatePromptStyle(true)
				payoutNightRewards()
				stopNightlyQuest()
			else
				local contract = getContractById(ActiveContractId.Value)
				if contract then
					startNightlyQuest(contract)
				end
				updatePromptStyle(false)
			end
			lastIsDay = isDay
		end

		if not isDay and questState.activeContract then
			updateDistanceTracking()
		end

		task.wait(1)
	end
end)

Players.PlayerAdded:Connect(function(player)
	questState.playerDistances[player] = 0
	questState.playerLastPositions[player] = nil
	questState.playerDied[player] = false
	registerDeathTracking(player)
end)

Players.PlayerRemoving:Connect(function(player)
	questState.playerDistances[player] = nil
	questState.playerLastPositions[player] = nil
	questState.playerDied[player] = nil
	local connection = questState.playerDeathConnections[player]
	if connection then
		connection:Disconnect()
		questState.playerDeathConnections[player] = nil
	end
