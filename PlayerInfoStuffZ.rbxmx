							<Properties>
								<ProtectedString name="Source"><![CDATA[local slendermanPage = workspace:FindFirstChild("Page")
if not slendermanPage then return end

local proximityPrompt = slendermanPage:FindFirstChild("ProximityPrompt")
if not proximityPrompt then return end

local part = slendermanPage:FindFirstChild("Part")
if not part then return end

local function onPromptTriggered(player)
    print("Page was collected by: " .. player.Name)
    
    -- Make the paper fully transparent
    part.Transparency = 1
    part.CanCollide = false
    
    local decal = part:FindFirstChild("Decal")
    if decal then
        decal.Transparency = 1
    end
    
    -- Disable the proximity prompt after collection
    proximityPrompt.Enabled = false
end

proximityPrompt.Triggered:Connect(onPromptTriggered)]]></ProtectedString>
